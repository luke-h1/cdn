# Cloudflare Account
CLOUDFLARE_ACCOUNT_ID=your-cloudflare-account-id

# R2 Bucket (for local development)
# In production, the Worker uses R2 bindings directly
R2_BUCKET_NAME=lho-cdn
R2_API_TOKEN=your-r2-api-token

# D1 Database (for local development)
# In production, the Worker uses D1 bindings directly
D1_DATABASE_ID=your-d1-database-id
D1_API_TOKEN=your-d1-api-token

# Public CDN URL (your custom domain)
NEXT_PUBLIC_CDN_URL=https://cdn.lhowsam.com
