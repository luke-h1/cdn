{
  "name": "cdn-admin",
  "version": "0.1.0",
  "private": true,
  "packageManager": "bun@1.1.0",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "eslint . --max-warnings 0",
    "create-user": "cd terraform && terraform init -input=false && terraform apply -auto-approve -input=false",
    "delete-user": "cd terraform && terraform destroy -auto-approve -input=false",
    "user-outputs": "cd terraform && terraform output",
    "user-secret": "cd terraform && terraform output -raw secret_access_key"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.700.0",
    "next": "^16.0.0",
    "react": "^19.0.0",
    "react-dom": "^19.0.0"
  },
  "devDependencies": {
    "@types/node": "^22",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "autoprefixer": "^10.4.20",
    "eslint": "^9",
    "eslint-config-next": "^16.0.0",
    "postcss": "^8",
    "tailwindcss": "^3.4.14",
    "typescript": "^5"
  }
}
